{
  "name": "@stillmine/react-components",
  "version": "0.2.0",
  "description": "React components.",
  "keywords": [
    "ui",
    "react",
    "components"
  ],
  "homepage": "https://stillmine-component-docs.netlify.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/stillmine/packages.git"
  },
  "license": "MIT",
  "main": "src/mod.ts",
  "scripts": {
    "build": "concurrently \"rimraf esm lib\" \"rollup -c\" \"tsc --emitDeclarationOnly -p tsconfig.build.json\"",
    "build:storybook": "build-storybook",
    "develop:storybook": "start-storybook -p 6006",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "yarn lint --fix",
    "prepack": "yarn build",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/auto-id": "0.15.0",
    "@stillmine/hooks": "workspace:packages/hooks",
    "@stillmine/styles": "workspace:packages/styles",
    "@stillmine/units": "workspace:packages/units",
    "@stillmine/utils": "workspace:packages/utils",
    "react-error-boundary": "3.1.3",
    "react-use": "17.2.4"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@emotion/babel-plugin": "11.3.0",
    "@emotion/react": "11.4.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@stillmine/types": "workspace:packages/types",
    "@storybook/addon-a11y": "6.2.9",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-knobs": "6.2.9",
    "@storybook/addon-options": "5.3.21",
    "@storybook/react": "6.2.9",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@types/jest": "26.0.23",
    "@types/react": "17.0.6",
    "@types/react-dom": "17.0.5",
    "@types/testing-library__jest-dom": "5.9.5",
    "concurrently": "6.1.0",
    "core-js": "3.12.1",
    "eslint": "7.26.0",
    "facepaint": "1.2.1",
    "jest": "26.6.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.48.0",
    "sanitize.css": "12.0.1",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "@emotion/react": ">= 11",
    "@emotion/styled": ">= 11",
    "facepaint": ">= 1",
    "react": ">= 17",
    "react-dom": ">= 17"
  },
  "publishConfig": {
    "main": "lib/mod.js",
    "module": "esm/mod.js",
    "types": "lib/mod.d.ts",
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
