{
  "name": "@stillmine/react-components",
  "version": "0.4.2",
  "description": "React components.",
  "keywords": [
    "ui",
    "react",
    "components"
  ],
  "homepage": "https://stillmine-component-docs.netlify.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/stillmine/packages.git"
  },
  "license": "MIT",
  "main": "lib/mod.js",
  "module": "esm/mod.js",
  "types": "lib/mod.d.ts",
  "scripts": {
    "build": "concurrently \"rimraf esm lib\" \"rollup -c\" \"tsc --emitDeclarationOnly -p tsconfig.build.json\"",
    "build:storybook": "build-storybook",
    "develop:storybook": "start-storybook -p 6006",
    "lint": "eslint src/**/*.{ts,tsx}",
    "prepack": "pnpm run build",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/auto-id": "0.16.0",
    "@stillmine/hooks": "workspace:*",
    "@stillmine/styles": "workspace:*",
    "@stillmine/units": "workspace:*",
    "@stillmine/utils": "workspace:*",
    "react-error-boundary": "3.1.3",
    "react-use": "17.3.1"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.4.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@storybook/addon-a11y": "^6.3.9",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/react": "^6.3.9",
    "core-js": "^3.18.1",
    "facepaint": "^1.2.1",
    "react-dom": "^17.0.2",
    "react": "^17.0.2",
    "rollup": "^2.58.0",
    "sanitize.css": "^13.0.0"
  },
  "peerDependencies": {
    "@emotion/react": ">= 11",
    "@emotion/styled": ">= 11",
    "facepaint": ">= 1",
    "react": ">= 17",
    "react-dom": ">= 17"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
