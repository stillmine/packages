    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@stillmine/utils": "workspace:packages/utils"
  },
  "devDependencies": {
    "@emotion/react": "11.1.5",
    "@stillmine/types": "workspace:packages/types",
    "@types/facepaint": "1.2.1",
    "@types/react": "17.0.3",
    "concurrently": "6.1.0",
    "core-js": "3.12.1",
    "jest": "26.6.3",
    "rimraf": "3.0.2",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "facepaint": ">= 1",
    "react": ">= 17"
  },
  "publishConfig": {
    "main": "lib/mod.js",
    "module": "esm/mod.js",
    "types": "lib/mod.d.ts",
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
