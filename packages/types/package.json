{
  "name": "@stillmine/types",
  "version": "0.1.1",
  "description": "utility types",
  "repository": {
    "type": "git",
    "url": "https://github.com/stillmine/packages.git"
  },
  "license": "MIT",
  "main": "src/mod.ts",
  "scripts": {
    "build": "concurrently \"rimraf esm lib\" \"tsc -p tsconfig.cjs.json\" \"tsc -p tsconfig.esm.json\"",
    "lint": "eslint src/**",
    "lint:format": "prettier \"src/**/*.{ts|tsx}\" --list-different",
    "prepack": "yarn build",
    "test": "jest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/react": "17.0.3",
    "concurrently": "5.3.0",
    "rimraf": "3.0.2",
    "typescript": "4.1.3"
  },
  "publishConfig": {
    "main": "lib/mod.js",
    "module": "esm/mod.js",
    "types": "lib/mod.d.ts",
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  }
}
