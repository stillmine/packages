{
  "name": "@stillmine/utils",
  "version": "0.2.5",
  "description": "Utils",
  "repository": {
    "type": "git",
    "url": "https://github.com/stillmine/packages.git"
  },
  "license": "MIT",
  "main": "lib/mod.js",
  "module": "esm/mod.js",
  "types": "lib/mod.d.ts",
  "scripts": {
