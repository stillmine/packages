{
  "name": "@stillmine/validator",
  "version": "0.4.2",
  "description": "Validator",
  "repository": {
    "type": "git",
    "url": "https://github.com/stillmine/packages.git"
  },
  "license": "MIT",
  "main": "lib/mod.js",
  "module": "esm/mod.js",
  "types": "lib/mod.d.ts",
  "scripts": {
    "build": "concurrently \"rimraf esm lib\" \"tsc -p tsconfig.cjs.json\" \"tsc -p tsconfig.esm.json\"",
